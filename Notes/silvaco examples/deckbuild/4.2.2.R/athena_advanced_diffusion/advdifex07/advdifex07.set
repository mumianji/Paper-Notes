# TPCS version: 7
# various properties
log label 0
key opaque 0
mark frequency 0
mark overlay 0
graph width 0 2
graph width 1 1
graph width 2 1
graph width 3 0
graph width 4 1
graph width 5 0
sequence size 1
sequence width 0
log zero 0
sequence mark 0 0
sequence mark 1 1
sequence mark 2 2
sequence mark 3 3
sequence mark 4 4
sequence mark 5 5
# Functions
function 1 "" ""
function 2 "" ""
function 3 "" ""
function 4 "" ""
select 1
# xsection flags
show mesh off
show edges on
show materials on
show points on
show lines on
# xsection impurities
xsection impurity none
xsection impurity "Arsenic"
# key positions
key contours 1 at 3
key contours 2 at 1
key contours 3 at 6
key electrical at user 0.637708, 0.238494
key vectors at 6
key overlay at user 0.349353, 0.0188285
key regions at 4
key apply 
# annotation, titles
title main "Arsenic Diffusion after implantation"
title sub "Low energy implant + spike anneal"
# annotation, footers
footer right none
footer left none
# annotation, show
show xaxis on
show yaxis on
show grid off
show label xaxis on
show label yaxis on
# annotation, axes
range xaxis 0, 0.05 increment 0.01,4
range yaxis 17, 22 increment 2,9
label xaxis "Depth (um)"
label yaxis "As Concentration (cm-3)"
# zoom
zoom from 0, 0 scale 1, 1
# user labels
# overlay level names
label overlay 0 "As-implanted (As-2keV-2e15cm-3)"
label overlay 1 "As-implanted (simulation)"
label overlay 2 "Spike anneal (950C - 50C/s)"
label overlay 3 "Spike anneal (simulation)"
