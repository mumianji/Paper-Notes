go athena

#TITLE: BCA and SVDP Models  and Experiment

#the x dimension definition
line x loc = 0.0 
line x loc = 0.25  spacing=0.25

#the vertical definition
line y loc = 0     spacing = 0.005
line y loc = 0.3  spacing = 0.005

#initialize the mesh
init silicon

#  
deposit oxide thick=0.001 div=2 
# First run BCA model
implant sampling boron energy=10 dose=9.27e12 tilt=0  bca divergence=0.1 n.ion=5000 
structure outfile=aniiex14_1.str remove.gas
#
extract name="aa" curve(depth,impurity="Boron" material="Silicon" \
        mat.occno=1 x.val=0) outfile="aniiex14_1.dat"


#the x dimension definition
line x loc = 0.0 
line x loc = 0.25  spacing=0.25

#the vertical definition
line y loc = 0     spacing = 0.005
line y loc = 0.3  spacing = 0.005

#initialize the mesh
init silicon

#  
deposit oxide thick=0.001 div=2 
# Run SVDP for comparison
implant boron energy=10 dose=9.27e12 tilt=0  pears s.ox=0.001 
structure outfile=aniiex14_2.str 
#
extract name="aa" curve(depth,impurity="Boron" material="Silicon" \
        mat.occno=1 x.val=0) outfile="aniiex14_2.dat"

tonyplot -overlay aniiex14_1.dat aniiex14_2.dat aniiex14.exp -set aniiex14.set
 
