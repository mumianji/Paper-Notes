go athena

#
line x loc=0.0 spac=0.1 
line x loc=0.2 spac=0.006
line x loc=0.4 spac=0.006
line x loc=0.5 spac=0.01 
#
line y loc=0.0 spac=0.002 
line y loc=0.2 spac=0.005
line y loc=0.5 spac=0.05
line y loc=0.8 spac=0.15 
#
init orientation=100 space.mul=3

deposit oxide thickness=0.01 div=2

depo poly thick=0.2 divi=10 

etch poly left p1.x=0.35

structure mirror right

# stretch the structure to the right maintaining the previous coordinate system
# this new command makes it simpler to predict a stretched regions coordinates
# for electrode name placement 

structure outf=anmiex05_0.str

stretch poly div=10 length=1.0 

structure outf=anmiex05_1.str

tonyplot anmiex05_0.str -add anmiex05_1.str
