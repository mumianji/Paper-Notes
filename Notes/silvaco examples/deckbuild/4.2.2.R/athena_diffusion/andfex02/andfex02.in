go athena

# OED of Boron

#the x dimension definition
line x loc = 0.0  spacing=0.1
line x loc = 0.1  spacing=0.1

#the vertical definition
line y loc = 0     spacing = 0.02 
line y loc = 2.0   spacing = 0.20 
line y loc = 25.0  spacing = 2.5 

#initialize the mesh
init silicon c.phos=1.0e14

#perform uniform boron implant
implant boron dose=1e13 energy=70

#set diffusion model for OED
method two.dim

#perform diffusion
diffuse time=30 temperature=1000 dryo2
#
extract name="xj_two.dim" xj silicon mat.occno=1 x.val=0.0 junc.occno=1

#save the structure
structure outfile=andfex02_0.str

# repeat the simulation with default FERMI model
go athena

#TITLE: Simple Boron Anneal

#the x dimension definition
line x loc = 0.0  spacing=0.1
line x loc = 0.1  spacing=0.1

#the vertical definition
line y loc = 0     spacing = 0.02 
line y loc = 2.0   spacing = 0.20 
line y loc = 25.0  spacing = 2.5 

#initialize the mesh
init silicon c.phos=1.0e14

#perform uniform boron implant
implant boron dose=1e13 energy=70

#select diffusion model
method fermi 

#perform diffusion
diffuse time=30 temperature=1000 dryo2
#
extract name="xj_fermi" xj silicon mat.occno=1 x.val=0.0 junc.occno=1


#save the structure
structure outfile=andfex02_1.str

# compare diffusion models
tonyplot -overlay andfex02_0.str andfex02_1.str -set andfex02.set
