go athena

#TITLE: arsenic poly diffusion

line x loc = 0.0  spacing=0.1
line x loc = 0.5  spacing=0.1
line x loc = 2.5  spacing=0.1
line x loc = 3.0  spacing=0.1

#the vertical definition
line y loc = 0     spacing = 0.02
line y loc = 0.20  spacing = 0.04
line y loc = 1.0   spacing = 0.5

#initialize the mesh
init silicon

deposit nitride thick=.5 div=10
etch nitride start x=.5  y=-10
etch nitride cont  x=.5  y=10
etch nitride cont  x=2.5 y=10
etch nitride done  x=2.5 y=-10

deposit poly thick=0.2 div=20

#perform uniform arsenic implant
implant arsenic dose=3e15 energy=20 tilt=10
#perform diffusion using the default model
diffuse time=10.0 temperature=900

structure outfile=andfex08_1.str


line x loc = 0.0  spacing=0.1
line x loc = 0.5  spacing=0.1
line x loc = 2.5  spacing=0.1
line x loc = 3.0  spacing=0.1

#the vertical definition
line y loc = 0     spacing = 0.02
line y loc = 0.20  spacing = 0.04
line y loc = 1.0   spacing = 0.5

#initialize the mesh
init silicon

deposit nitride thick=.5 div=10
etch nitride start x=.5  y=-10
etch nitride cont  x=.5  y=10
etch nitride cont  x=2.5 y=10
etch nitride done  x=2.5 y=-10

method poly.diff
deposit poly thick=0.2 div=20 gr.size=0.01

#perform uniform arsenic implant
implant arsenic dose=3e15 energy=20 tilt=10

#perform diffusion with the advanced polysilicon diffusion model
diffuse time=10.0 temperature=900

structure outfile=andfex08_2.str

tonyplot andfex08_*.str -set andfex08.set


